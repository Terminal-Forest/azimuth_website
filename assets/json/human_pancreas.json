{
	"version": "1.0.0",
	"name": "Human Pancreas",
	"datasets": [
		{
			"uid": "human-pancreas",
			"name": "Human Pancreas",
			"files": [
				{
					"type": "cells",
					"fileType": "anndata-cells.zarr",
					"url": "https://vitessce-references.s3.us-east-2.amazonaws.com/human-pancreas/vitessce_ref.zarr",
					"options": {
						"mappings": {
							"UMAP": {
							  "key": "obsm/X_umap",
							  "dims": [0,1]
							}
						},
						"factors": [
						  "obs/annotation.l1"
						]
					}
				},
				{
					"type": "cell-sets",
					"fileType": "anndata-cell-sets.zarr",
					"url": "https://vitessce-references.s3.us-east-2.amazonaws.com/human-pancreas/vitessce_ref.zarr",
					"options": [
						{
							"groupName": "annotation.l1",
							"setName": "obs/annotation.l1"
						}
					]
				},
				{
					"type": "expression-matrix",
					"fileType": "anndata-expression-matrix.zarr",
					"url": "https://vitessce-references.s3.us-east-2.amazonaws.com/human-pancreas/vitessce_ref.zarr",
					"options": {
					  "matrix": "X"
					}
				}
			]
		}
	],
	"initStrategy": "auto",
	"coordinationSpace": {
		"embeddingType": {
			"UMAP": "UMAP"
